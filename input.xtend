import java.util.Scanner


class PlayList {

	def static void main(String[] args) {
	// Taking user input.
	val in=new Scanner(System.in)
	val a=in.nextInt();
	println(a);
	
	
	
	
	//Adding integers.
	val in=new Scanner(System.in)
	val a=in.nextInt();
	val b=in.nextInt();
	println(a+b);
	
	//Adding Floats
	val in=new Scanner(System.in)
	val a=in.nextFloat();
	val b=in.nextFloat();
	println(a+b);
		

		
}

}	

